[run]
source = app
omit =
    */tests/*
    */test_*
    */migrations/*
    */alembic/*
    */venv/*
    */env/*
    */__pycache__/*
    */conftest.py
    */setup.py
    */manage.py
    */celery_app.py
    */tasks/*
    */scripts/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
title = Forex Bot Test Coverage Report
show_contexts = True

[xml]
output = coverage.xml
